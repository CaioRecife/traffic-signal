int estado;

void setup() {
  // put your setup code here, to run once:
  pinMode(7, INPUT_PULLUP);
  pinMode(8, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  estado = digitalRead(7);
  
  if(estado == LOW) {
    digitalWrite(8, HIGH);
    delay(300);
    digitalWrite(8, LOW);
  
  }
}
